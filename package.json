  "main": "build/src/server.js",
  "scripts": {
    "format": "prettier --write src/**/*.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "jasmine": "npm run build && npm run jasmine",
    "jasmineSilent": "npm run build && npm run jasmine --silent",
    "build": "npx tsc",
    "start": "npm run build && nodemon src/server.ts",
    "migrate": "db-migrate",
    "test": "SET ENV=test && npx tsc && db-migrate --env test up && jasmine && db-migrate --env test down ",
    "test-env-up": "SET ENV=test && npx tsc && db-migrate --env test up",
    "reset-test-env": "db-migrate --env test down",
    "testSilent": "SET ENV=test && npx tsc && db-migrate --env test up && jasmineSilent && db-migrate db:drop test"
  },
